{# @var products \inklabs\kommerce\EntityDTO\ProductDTO[] #}
<table class="table table-condensed">
    <thead>
        <tr>
            {% include '@admin/product/parts/product-list-head.twig' %}
            {% block thead %}{% endblock %}
        </tr>
    </thead>
    <tbody>
        {% for product in products %}
            <tr>
                {% include '@admin/product/parts/product-list-body.twig' %}
                {% block tbody %}{% endblock %}
            </tr>
        {% else %}
            <tr>
                <td colspan="100%">
                    <span class="text-muted">No products.</span>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
